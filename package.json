{
  "name": "@xqsit94/vue3-copy-to-clipboard",
  "description": "Copy to clipboard component for Vue 3",
  "version": "1.2.0",
  "main": "./dist/vue3-copy-to-clipboard.umd.js",
  "module": "./dist/vue3-copy-to-clipboard.mjs",
  "exports": {
    ".": {
      "import": "./dist/vue3-copy-to-clipboard.mjs",
      "require": "./dist/vue3-copy-to-clipboard.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xqsit94/vue3-copy-to-clipboard.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "keywords": [
    "vue",
    "vue3",
    "copy to clipboard",
    "clipboard"
  ],
  "author": "xqsit94",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.2",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vue-tsc": "^0.39.5"
  }
